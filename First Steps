// Hello World

class HelloWorld {
	
	public static void main (String[] args){
		System.out.println("Hello World!");
	}
}






// Aluno Reset

class AlunoReset {
	public static void main(String[] args){
		System.out.println("Sou aluno do Level 1 do CWI Reset!");
	}
}






// Argumentos

class Argumentos {
	public static void main (String[] args){
		System.out.println("Você digitou " + args[0]);
	}
}






// Aluno com Parâmetros

class AlunoResetComParametro {
	public static void main (String[] args){
		System.out.println("Sou aluno do Level 1 do CWI Reset! Me chamo " + args[0]);
	}
}
